{
  "name": "image-squarer",
  "version": "0.4.1",
  "description": "A Quasar Framework app",
  "productName": "ImageSquarer",
  "author": "Michal DolenskÃ½",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron",
    "build": "quasar build -m electron",
    "kill-qusar-process": "taskkill /F /IM \"Quasar App.exe\"",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@quasar/cli": "^1.2.0",
    "@quasar/extras": "1.10.6",
    "chokidar": "3.5.1",
    "core-js": "3.11.0",
    "dotenv": "8.2.0",
    "electron-store": "8.0.0",
    "quasar": "^1.15.19",
    "sharp": "0.28.3",
    "vue-i18n": "8.24.5"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@quasar/app": "2.2.8",
    "babel-eslint": "10.1.0",
    "devtron": "1.4.0",
    "electron": "13.0.1",
    "electron-builder": "22.10.5",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.2.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-vue": "7.9.0",
    "husky": "6.0.0",
    "standard-version": "9.2.0"
  },
  "browserslist": [
    "last 5 Chrome version"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaldolensky/ImageSquarer.git"
  }
}
